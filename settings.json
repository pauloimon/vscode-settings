{
  "docker.containers.description": [
    "Status"
  ],
  "docker.containers.label": "ContainerName",
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "explicit"
  },
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.fontFamily": "JetBrains Mono",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.guides.bracketPairs": true,
  "editor.lineHeight": 2.5,
  "editor.minimap.enabled": false,
  "editor.renderLineHighlight": "all",
  "editor.renderWhitespace": "trailing",
  "editor.rulers": [
    {
      "column": 80,
      "color": "#333333"
    },
    {
      "column": 120,
      "color": "#333333"
    }
  ],
  "editor.smoothScrolling": true,
  "editor.stickyScroll.enabled": false,
  "errorLens.margin": "1rem",
  "errorLens.messageBackgroundMode": "none",
  "explorer.autoReveal": false,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "gitlens.currentLine.enabled": false,
  "javascript.preferences.importModuleSpecifier": "relative",
  "peacock.affectActivityBar": false,
  "peacock.affectSashHover": false,
  "peacock.affectTitleBar": false,
  "peacock.showColorInStatusBar": false,
  "projectManager.git.baseFolders": [
    "~/Projetos"
  ],
  "search.exclude": {
    "**/node_modules": false
  },
  "search.useIgnoreFiles": false,
  "tailwind-fold.autoFold": false,
  "tailwind-fold.foldStyle": "QUOTES",
  "tailwind-fold.showTailwindImage": false,
  "tailwind-fold.unfoldIfLineSelected": true,
  "terminal.integrated.enablePersistentSessions": false,
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.smoothScrolling": true,
  "thunder-client.defaultHeaders": [
    {
      "name": "Accept",
      "value": "application/json"
    }
  ],
  "thunder-client.editorFontSize": 14,
  "thunder-client.sidebar.defaultTab": "Collections",
  "thunder-client.sidebar.layout": "compact",
  "typescript.preferences.importModuleSpecifier": "relative",
  "workbench.list.smoothScrolling": true,
  "workbench.panel.defaultLocation": "right",
  "workbench.startupEditor": "none",
  "workbench.tree.indent": 20
}
